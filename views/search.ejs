<!DOCTYPE html>
<html>
  <head>
    <title></title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/css/bootstrap.min.css" rel="stylesheet"
     integrity="sha384-wEmeIV1mKuiNpC+IOBjI7aAzPcEZeedi5yW5f2yOq55WWLwNGmvvx4Um1vskeMj0" crossorigin="anonymous">
     <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js" 
     integrity="sha384-p34f1UUtsS3wqzfto5wAAmdvj+osOnFyQFpp4Ua3gs/ZVWx6oOypYoCJhGGScy+8" crossorigin="anonymous"></script>
    <script src='http://ajax.googleapis.com/ajax/libs/jquery/1.8.2/jquery.min.js'></script>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <script type="module" src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.esm.js"></script>
  <script nomodule src="https://unpkg.com/ionicons@5.5.2/dist/ionicons/ionicons.js"></script>
  </head>
  <body>
    <%- include('menu.ejs')  %> 
    <div class="container">
        <div class="accordion accordion-flush "; id="accordionExample">
          <% for(var i=0 ; i<title.length; i++){%>
          <div class="accordion-item " >
            <h2 class="accordion-header" id="heading<%= i %>">
              <button class=" accordion-button  text-light bg-* "; style="background-color:#6C6C6C"  type="button"
               data-bs-toggle="collapse" data-bs-target="#collapse<%= i %> " aria-expanded="false" aria-controls="collapse<%= i %> ">
                <div class="container">
                  <div class="d-flex">
                  </div>
                  <div class="d-inline-flex">
                    <h5><strong><%= title[i] %></strong></h5>
                  </div>
                  &emsp;
                  <div class="d-inline-flex">
                    <em><%= adress[i]%></em>
                  </div>
                  &emsp;
                  
                </div>
              </button>
            </h2>
            <div id="collapse<%= i  %>" class="accordion-collapse text-light collapse" style="background-color:#8E8E8E;"
             aria-labelledby="heading<%= i %>" data-bs-parent="#accordionExample">
              <div class="accordion-body">
                <div class="container">
                  <div class="d-flex">
                    <%= content[i]%>
                    <a href="<%= link[i] %>"><ion-icon size="large" name="logo-facebook"></ion-icon></a>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <% } %>     
        </div>
    </div>
  </body>
</html>
